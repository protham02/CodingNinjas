q1.
#include <iostream>
using namespace std;   
int main()
{
    double a = 6 / 4;
    int b  = 6 / 4;
    double c = a + b;
    cout << c;
}



q2.
#include <iostream>
using namespace std;  
int main()
{
    double a = 55.5;
    int b = 55;
    a = a % 10;
    b = b % 10;
    cout << a << " "  << b;
}



